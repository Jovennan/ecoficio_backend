<p id="notice"><%= notice %></p>

<h1>Orgaos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Endereco</th>
      <th>Chefe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orgaos.each do |orgao| %>
      <tr>
        <td><%= orgao.nome %></td>
        <td><%= orgao.endereco %></td>
        <td><%= orgao.chefe %></td>
        <td><%= link_to 'Show', orgao %></td>
        <td><%= link_to 'Edit', edit_orgao_path(orgao) %></td>
        <td><%= link_to 'Destroy', orgao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Orgao', new_orgao_path %>
