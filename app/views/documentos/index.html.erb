<p id="notice"><%= notice %></p>

<h1>Documentos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Descricao</th>
      <th>Titulo</th>
      <th>Brasao</th>
      <th>Autor</th>
      <th>Orgao</th>
      <th>Visibilidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos.each do |documento| %>
      <tr>
        <td><%= documento.nome %></td>
        <td><%= documento.descricao %></td>
        <td><%= documento.titulo %></td>
        <td><%= documento.brasao %></td>
        <td><%= documento.autor %></td>
        <td><%= documento.orgao %></td>
        <td><%= documento.visibilidade %></td>
        <td><%= link_to 'Show', documento %></td>
        <td><%= link_to 'Edit', edit_documento_path(documento) %></td>
        <td><%= link_to 'Destroy', documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento', new_documento_path %>
